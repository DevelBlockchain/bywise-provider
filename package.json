{
  "name": "@bywise/provider",
  "version": "1.1.1",
  "description": "Web3 provider of Bywise",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "files": [
    "lib/**/*",
    "README.md"
  ],
  "scripts": {
    "build": "tsc",
    "dev": "ts-node src/dev.ts",
    "format": "prettier --write \"src/**/*.ts\"",
