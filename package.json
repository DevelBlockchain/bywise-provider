    "bywise",
    "blockchain"
  ],
  "author": "Devel Blockchain",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/DevelBlockchain/bywise-provider/issues"
  },
  "homepage": "https://github.com/DevelBlockchain/bywise-provider#readme",
  "devDependencies": {
    "@types/base-64": "^1.0.0",
    "@types/jest": "^27.4.1",
    "jest": "^27.5.1",
    "prettier": "^2.6.0",
    "ts-jest": "^27.1.3",
    "ts-node": "^10.7.0",
    "tslint": "^5.12.1",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^4.6.2"
  },
  "dependencies": {
    "@bywise/web3": "^0.10.0",
    "base-64": "^1.0.0"
